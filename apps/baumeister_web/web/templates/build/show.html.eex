<h2>Show build</h2>

<ul>

  <li>
    <strong>Project:</strong>
    <%= @build.project_id %>
  </li>

  <li>
    <strong>Number:</strong>
    <%= @build.number %>
  </li>

  <li>
    <strong>Log:</strong>
    <%= @build.log %>
  </li>

  <li>
